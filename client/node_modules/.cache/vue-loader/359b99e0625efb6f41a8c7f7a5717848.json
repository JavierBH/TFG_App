  data(){\n        return {\n          newItem: '',\n          items: []\n        }\n    },\n     methods: {\n      addItem: function() {\n        if (this.items.length>0) {\n          this.items.push({\n          id: this.items[this.items.length-1].id + 1,\n          description: this.newItem,\n          completed: false,\n        });\n        } else {\n          this.items.push({\n          id: this.items.length + 1,\n          description: this.newItem,\n          completed: false,\n        });\n        }\n        this.newItem = '';\n      },\n     removeItem: function (itemID) {\n        this.items = this.items.filter((newItem) => newItem.id!== itemID);\n      } \n     }\n}\n</script>\n\n<style>\n.list-item{\n  display: flex;\n  justify-content: space-evenly;\n}\n.list-item > div{\noverflow:auto;\nmargin: 10px;\n}\n.todo-card{\n  display: flex;\n}\n</style>"]}]}